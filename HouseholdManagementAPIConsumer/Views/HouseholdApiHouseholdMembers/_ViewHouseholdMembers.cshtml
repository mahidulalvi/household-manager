@if(ViewBag.HasErrored != null && ViewBag.HasErrored == true)
{
    <p>No members found</p>
}

@if (Model.Any())
{
    for(var i = 0; i < Model.Count(); i++)
    {
        <p style="color: red; font-size: .7em">@Model[i].UserName</p>
        <p style="color: blue; font-size: .5em">@Model[i].Email</p>
    }
}
